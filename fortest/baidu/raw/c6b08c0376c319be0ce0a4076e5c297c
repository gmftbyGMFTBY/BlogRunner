<script type="text/javascript">
        $(function () {
            function scrollAd() {
                var offset = $(window).height() + $(document).scrollTop() - 1200;
                $('#ad_cover').animate({ top: offset + 100 }, { duration: 200, queue: false });
            }
            scrollAd();
            $(window).scroll(scrollAd);

            $("#ad_cover a.btn3").click(function () { $("#ad_cover").hide() })
        });

    </script>
