    setTimeout(function(){
    	if ($('#head-bottom').height() == 0){
        	_hmt.push(['_trackEvent', 'aaaddd-track', 'block-yes']);
    	}else{
		_hmt.push(['_trackEvent', 'aaaddd-track', 'block-no']);
	}
    }
    ,100);
});
</script>
